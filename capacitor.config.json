{
  "appId": "com.spp.vincievents",
  "appName": "Vinci Events",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "plugins": {
    "LocalNotifications": {
      "smallIcon": "res://ic_stat_icon"
    },
    "PushNotifications": {
      "presentationOptions": ["badge", "sound", "alert"]
    },
    "SplashScreen": {
      "launchShowDuration": 3000,
      "launchAutoHide": false,
      "backgroundColor": "#071A3D",
      "showSpinner": false
    }
  },
  "rootDir": "apps/vinci",
  "webDir": "../../dist/apps/vinci",
  "ios": {
    "platformDir": "apps/vinci/ios"
  },
  "android": {
    "platformDir": "apps/vinci/android"
  }
}
